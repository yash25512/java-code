import java.util.*;
class HelloWorld {
    public static void main(String[] args) {
        System.out.println("Hello, World!");
        
        ArrayList<Integer> list = new ArrayList<> ();
        list.add(2);
        list.add(3);
        list.add(7);
        list.add(4);
        list.add(2);
        
        System.out.println(list);
        Collections.sort(list);
        System.out.println(list);
        Collections.sort(list,Collections.reverseOrder());
        System.out.println(list);
    }
}
