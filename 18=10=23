class Solution {
    public String kthDistinct(String[] arr, int k) {
    

     HashMap<String,Integer> sap= new HashMap<>();
    for(String lal:arr){
        sap.put(lal,sap.getOrDefault(lal,0)+1);
    }
    int fdfs=0;
   for(String ting:arr){
       if(sap.get(ting)==1&&++fdfs==k){
           return ting;
       }
       
   }
   return "";


    }
}
