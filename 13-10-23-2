import java.util.Scanner;
public class Main{
    public static void main (String[] args){
        Scanner sc= new Scanner(System.in);
        String str=sc.nextLine();
        int count=0;
        char search='a';
        for(int i=0;i<str.length();i++){
            if(str.charAt(i)==search){
                count++;
            }
        }
        System.out.println(count);
    }
}
