class HelloWorld {
    public static void main(String[] args) {
        int []arr={1,2,4,6,8,2,4,};
        for(int i=0;i<arr.length;i++){
            for(int j=i+1;j<arr.length;j++){
                if(arr[i]==arr[j]){
                    System.out.println("number"+arr[i]);
                }
            }
        }
    }
}
